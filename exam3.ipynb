   "source": [
    "# 데코레이터 : 기존 함수를 고치지 않고 기능만 덧붙이는 방법\n",
    "\n",
    "def deco(fun):\n",
    "    def wrap():\n",
    "        print('실행 전')\n",
    "        fun()\n",
    "        print('실행 후')\n",
    "    return wrap    \n",
    "\n",
    "def hello():\n",
    "    print('안녕하세요')\n",
    "\n",
    "new_fun = deco(hello)\n",
    "new_fun()\n",
    "\n",
    "@deco\n",
    "def hello():\n",
    "    print('안녕하세요')\n",
    "\n",
    "hello()\n"
   ]
  }
